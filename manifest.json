{
    "manifest_version":3,
    "name":"Gloabl Entry Jobs",
    "description": "Find the global entry interviews that meet your selection criteria",
    "version":"1.0.0",
    "action":{
        "default_popup":"popup/popup.html"
    },
    "icons":{
       "16":"icons/logo-16.png", 
       "19":"icons/icon-19.png", 
       "32":"icons/logo-32.png", 
       "48":"icons/icon-48.png", 
       "128":"icons/logo-128.png" 
    },
    "background":{
        "service_worker":"background.js",
        "type":"module"
    },
    "permissions":[
        "storage","alarms"
    ]
    
}